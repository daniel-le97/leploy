{
  "name": "leploy",
  "type": "module",
  "version": "0.0.1",
  "description": "A platform as a service for deploying and managing your applications built with bun",
  "bin": {
    "leploy": "bin/leploy.ts"
  },
  "scripts": {
    "build": "bun --bun nuxt build",
    "dev": "bun --bun nuxt dev",
    "start": "bun .output/server/index.mjs",
    "generate:nuxt": "nuxt generate",
    "postinstall": "bun --bun nuxt prepare",
    "lint": "bun --bun eslint .",
    "lint:fix": "bun --bun eslint . --fix",
    "remove:db": "rm sqlite.db*",
    "reset:workspace": "rm -rf node_modules && rm -rf .nuxt && rm bun.lockb && bun i",
    "reset": "bun run remove:db && bun test.ts",
    "upgrade": "bun upgrade --canary && bun --version && npx taze major -w && bun i",
    "bench": "bombardier -c 125 -n 100000 http://localhost:3000"
  },
  "dependencies": {
    "@types/js-yaml": "^4.0.9",
    "drizzle-orm": "^0.29.3",
    "js-yaml": "^4.1.0",
    "nanotar": "^0.1.1",
    "nitropack": "^2.8.1",
    "xterm": "^5.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@nuxt/devtools": "^1.0.8",
    "@nuxt/ui": "latest",
    "@types/bun": "^1.0.4",
    "@vueuse/nuxt": "latest",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.56.0",
    "nuxt": "npm:nuxt-nightly@latest",
    "nuxt-auth-utils": "latest",
    "smee-client": "^2.0.1",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  }
}
