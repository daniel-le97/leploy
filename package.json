{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "bun --bun nuxt build",
    "dev": "bun --bun nuxt dev",
    "start": "bun .output/server/index.mjs",
    "generate:nuxt": "nuxt generate",
    "postinstall": "bun --bun nuxt prepare",
    "lint": "bun --bun eslint .",
    "lint:fix": "bun --bun eslint . --fix",
    "remove:db": "rm sqlite.db*",
    "reset": "bun run remove:db && bun test.ts",
    "upgrade": "bun upgrade --canary && bun --version && bunx taze major -w && bun i",
    "bench": "bombardier -c 125 -n 100000 http://localhost:3000"
  },
  "dependencies": {
    "drizzle-orm": "^0.29.3",
    "nuxt-auth-utils": "latest",
    "probot": "^12.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@nuxt/devtools": "^1.0.6",
    "@nuxt/ui": "latest",
    "@types/bun": "^1.0.0",
    "@vueuse/nuxt": "latest",
    "drizzle-kit": "^0.20.10",
    "eslint": "^8.56.0",
    "nuxt": "npm:nuxt-nightly@latest",
    "vue": "^3.4.7",
    "vue-router": "^4.2.5"
  }
}
